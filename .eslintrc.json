  {
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-console":"off",
        "prettier/prettier": ["error"],
        "no-undef": "off",
        "func-names": "off",
        "no-loop-func": "off",
        "no-param-reassign": ["error", { "props": false }],
        "no-await-in-loop":"off",
        "import/no-cycle":"off"
    },
    "overrides": [
      {
        "files": ["tests/**/*"],
        "env": {
          "jest": true
        }
      }
    ]
    
    
  }